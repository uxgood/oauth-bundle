{
    "name": "uxgood/oauth-bundle",
    "type": "symfony-bundle",
    "homepage": "https://github.com/uxgood/oauth-bundle",
    "description": "Support for authenticating users using both OAuth1.0a and OAuth2 in Symfony.",
    "license": "MIT",
    "keywords":    [
    ],

    "authors": [
        {
            "name": "UxGood",
            "email": "uxgood@hotmail.com"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/uxgood/oauth-bundle/contributors"
        }
    ],

    "require": {
        "php": "^5.6|^7.0",
        "symfony/framework-bundle":       "^2.8|^3.0|^4.0",
        "symfony/security-bundle":        "^2.8|^3.0|^4.0",
        "symfony/yaml":                   "^2.8|^3.0|^4.0",

        "psr/http-message":               "^1.0",
        "php-http/client-implementation": "^1.0",
        "php-http/httplug":               "^1.0",
        "php-http/client-common":         "^1.3",
        "php-http/message-factory":       "^1.0",
        "php-http/discovery":             "^1.0"
    },

    "require-dev": {
        "symfony/stopwatch":            "^2.8|^3.0|^4.0",
        "php-http/curl-client":         "^1.0",
        "php-http/httplug-bundle":      "^1.7"
    },

    "conflict": {
    },

    "suggest": {
    },

    "autoload": {
        "psr-4": {
            "UxGood\\Bundle\\OAuthBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },

    "autoload-dev": {
        "psr-4": {
            "UxGood\\Bundle\\OAuthBundle\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
